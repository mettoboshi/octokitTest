      <cc:CookbookURL>http://example.com/cookbook_url</cc:CookbookURL>
      <cc:Recipe>default</cc:Recipe>
      <cc:Import filetype="chef">parameters/New Role ID.rb</cc:Import>
    </cc:Role>
  </cc:Roles>
  <cc:FloatingIPs>
    <cc:FloatingIP id="new_floating_ip_id">
